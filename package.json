{
  "name": "soap",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "tsc",
    "start": "node out/index.js",
    "dev": "tsx watch --clear-screen=false src/bot.ts 2>&1 | tee /dev/stderr",
    "dev:debug": "tsx watch --inspect src/bot.ts",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "check": "npm run typecheck && npm run lint",
    "fix": "npm run lint:fix && npm run format",
    "deploy": "npx prisma generate && npm run build && npx prisma migrate deploy && npm run start",
    "prepare": "husky"
  },
  "author": "SaltySkyPie#3737",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.1.0",
    "chalk": "^5.3.0",
    "dayjs": "^1.11.19",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.7",
    "jimp": "^0.22.12",
    "ms": "^2.1.3",
    "mysql2": "^3.15.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tsconfig/recommended": "^1.0.7",
    "@types/ms": "^2.1.0",
    "@types/node": "^22.10.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unused-imports": "^4.3.0",
    "husky": "^9.1.7",
    "prettier": "^3.7.4",
    "prisma": "^6.1.0",
    "tsx": "^4.21.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.49.0"
  },
  "type": "module"
}
